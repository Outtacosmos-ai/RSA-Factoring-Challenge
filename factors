#!/usr/bin/env python3

import sys
import math

def factorize_number(number):
    """Factorize a number into a product of two smaller numbers."""
    for i in range(2, int(math.sqrt(number)) + 1):
        if number % i == 0:
            return f"{number}={i}*{number//i}"

    # If the number is prime, return itself as a factorization
    return f"{number}={number}*1"

def main(filename):
    """Main function to read numbers from a file and factorize them."""
    with open(filename, 'r') as file:
        for line in file:
            number = int(line.strip())
            factorization = factorize_number(number)
            print(factorization)

if __name__ == "__main__":
    if len(sys.argv) != 2:
        print("Usage: factors <file>")
        sys.exit(1)

    filename = sys.argv[1]
    main(filename)
